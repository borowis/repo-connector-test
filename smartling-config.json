{
    "locales" : [
         {
             "smartling" : "uk-UA",
             "application" : "uk-ua"
         }
     ],
     "resourceSets": [
         {
             "pathRegex" : "/messages/en/(?<path>.*)\\.strings",
             "translationPathExpression" : "/messages/ua/${path}_${locale}.${originalFile.extension}",
             "translationCommitMessage" : "Task TRN-18 translate ${originalFile.name} to ${locale}\n${translatedFile.fullName}",
             "type" : "ios",
             "authorizeContent" : true
         }
     ]
} 

